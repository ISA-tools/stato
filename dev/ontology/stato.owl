                                                                                            "http://en.wikipedia.org/wiki/Bayesian_information_criterion"@en ;
                                               <http://purl.obolibrary.org/obo/STATO_0000032> "BIC"@en ,
                                                                                              "SBIC"@en ,
                                                                                              "Schwartz's Baeysian information criterion"@en ;
                                               <http://purl.obolibrary.org/obo/STATO_0000041> ""@en ;
                                               rdfs:label "Bayesian information criterion"@en .


